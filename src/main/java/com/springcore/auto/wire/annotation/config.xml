<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="
           http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">
	<context:annotation-config />

	<!-- 1.by name,by type -->

	<bean class="com.springcore.auto.wire.annotation.Address"
		name="temp">

		<property name="street"
			value="9th cross old post office road" />

		<property name="city" value="kunigal" />

	</bean>


	<bean class="com.springcore.auto.wire.annotation.Address"
		name="address2">

		<property name="street" value="9th cross " />

		<property name="city" value="tumkur" />

	</bean>


	<bean class="com.springcore.auto.wire.annotation.Employee"
		name="employee" />
</beans>
